
@{
    ViewData["Title"] = "Index";
}
@model PopravniIspitIndexVM

    <br />
<form asp-action="Prikaz">
    <label>Školska godina: </label>
    <select class="form-control" asp-for="SkolskaGodinaId" asp-items="@Model.SkolskeGodine"></select>
    <label>Škola: </label>
    <select class="form-control" asp-for="SkolaId" asp-items="@Model.Skole"></select>
    <label>Predmet: </label>
    <select class="form-control" asp-for="PredmetId" asp-items="@Model.Predmeti"></select>
    <br />
    <input type="submit" value="Odaberi" class="btn btn-primary"/>
</form>

