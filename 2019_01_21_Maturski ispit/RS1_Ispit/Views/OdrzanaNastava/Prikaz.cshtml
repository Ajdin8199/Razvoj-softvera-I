@model OdrzanaNastavanPrikaziVM
<br />
<table class="table table-bordered text-center">
    <thead>
        <tr>
            <th>Datum</th>
            <th>Škola</th>
            <th>Predmet</th>
            <th>Nisu pristupili učenici</th>
            <th>Akcija</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.rows)
        {
            <tr>
                <td>@x.Datum</td>
                <td>@x.Skola</td>
                <td>@x.Predmet</td>
                <td>
                    @foreach (var y in x.NisuPristupili)
                    {
                        <span>@y ,</span>
                    }
                </td>
                <td><a asp-action="Uredi" asp-route-MaturskiIspitId="@x.MaturskiIspitId">Uredi</a></td>
            </tr>
        }
    </tbody>
</table>
<br />
<a asp-action="Dodaj" asp-route-NastavnikId="@Model.NastavnikId" class="btn btn-primary">Dodaj</a>